{
    "name": "stajor/readability",
    "description": "Mozilla Readability bridge to PHP",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "php",
        "readability"
    ],
    "authors": [
        {
            "name": "Alex B"
        }
    ],
    "require": {
        "php": ">=7.4",
        "ext-json": "*",
        "ext-pcntl": "*",
        "symfony/process": "^5.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.4"
    },
    "autoload": {
        "psr-4": {
            "Readability\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Readability\\Test\\": "tests/"
        },
        "classmap": [
            "tests/"
        ]
    },
    "scripts": {
        "test": "phpunit tests"
    }
}
